{
  "parser": "babel-eslint",
  "plugins": [
    "import",
  ],
  "rules": {
    "comma-dangle": ["error", {
      "arrays": "only-multiline",
      "objects": "only-multiline",
      "imports": "only-multiline",
      "exports": "only-multiline",
      "functions": "ignore"
    }],
    "import/no-extraneous-dependencies": "off",
    "import/prefer-default-export": "off",
    "class-methods-use-this": "off",
    "max-len": "off"
  },
  "extends": [
    "airbnb-base",
  ],
  "globals": {
    "window": true,
  }
}
